        "            SimpleField(name=\"city\", type=SearchFieldDataType.String),\r\n",
        "        ], collection=True)\r\n",
        "    ]\r\n",
        "    cors_options = CorsOptions(allowed_origins=[\"*\"], max_age_in_seconds=60)\r\n",
